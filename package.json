{
  "name": "sequelize-model-comments",
  "version": "0.0.4",
  "description": "Add comments to your Sequelize models.",
  "bugs": {
    "url": "https://github.com/lijoantony/sequelize-model-comments/issues",
    "email": "lijozom@gmail.com"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3"
  },
  "scripts": {
    "test": "mocha test/* --reporter dot --compilers js:babel-register --recursive",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lijoantony/sequelize-model-comments.git"
  },
  "private": false,
  "keywords": [
    "sequelize",
    "sequelize-plugin",
    "comments",
    "model comments",
    "nodejs"
  ],
  "author": {
    "name": "Lijo Antony",
    "email": "lijozom@gmail.com",
    "url": "https://github.com/lijoantony"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "app-module-path": "^1.0.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-flowtype": "^2.2.6",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "mversion": "^1.10.1",
    "pretty-error": "^2.0.0",
    "sqlite3": "^3.1.3"
  },
  "dependencies": {
    "lodash": "^4.11.1",
    "sequelize": "^3.21.0"
  }
}
